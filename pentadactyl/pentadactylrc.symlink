"hg6830

loadplugins '\.(js|penta)$'
group user
" [auto completion](http://5digits.org/help/pentadactyl/options.xhtml#:set)
" ordered by first to last
" set complete=lbShfs

set complete=lbhSfs
set guioptions=CNbrsc
set hintkeys=asdfg;lkjh
set verbose=2
set visualbell
set editor='/usr/local/bin/mvim -f +<line> +"sil! call cursor(0, <column>)" <file>'
set defsearch=google
